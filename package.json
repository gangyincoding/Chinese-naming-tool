{
  "name": "chinese-naming-tool",
  "version": "1.0.0",
  "type": "module",
  "description": "基于中国古典文化的智能宝宝起名工具",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "extract-pdf": "node scripts/extractPdf.cjs",
    "download-data": "node scripts/downloadData.cjs"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "element-plus": "^2.6.1",
    "gsap": "^3.12.5",
    "lunar-javascript": "^1.6.12",
    "pdf-parse-fork": "^1.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vue-tsc": "^2.0.6"
  }
}
